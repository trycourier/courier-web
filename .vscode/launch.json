{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "CourierJS Tests",
      "program": "${workspaceFolder}/@trycourier/courier-js/node_modules/.bin/jest",
      "args": [
        "--config",
        "${workspaceFolder}/@trycourier/courier-js/jest.config.js"
      ],
      "cwd": "${workspaceFolder}/@trycourier/courier-js",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Vanilla JS Example",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "start"
      ],
      "cwd": "${workspaceFolder}/examples/vanilla-js",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Vanilla TS Example",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "start"
      ],
      "cwd": "${workspaceFolder}/examples/vanilla-ts",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}