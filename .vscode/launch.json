{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Web JS",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "start"
      ],
      "cwd": "${workspaceFolder}/examples/web-js",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run React TS",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "dev"
      ],
      "cwd": "${workspaceFolder}/examples/react-ts",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Vanilla JS",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "start:js"
      ],
      "cwd": "${workspaceFolder}/examples/vanilla",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Vanilla TS",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "start:ts"
      ],
      "cwd": "${workspaceFolder}/examples/vanilla",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Refresh Packages",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "refresh"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "CourierJS Tests",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "test"
      ],
      "cwd": "${workspaceFolder}/@trycourier/courier-js",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Localhost QR",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "ipqr"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
    }
  ]
}