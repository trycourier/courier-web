{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Web",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "workspace",
        "web-js",
        "run",
        "dev"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Next Latest",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "workspace",
        "next-latest",
        "run",
        "dev"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Next 12",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "workspace",
        "next-12",
        "run",
        "dev"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run React Latest",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "workspace",
        "react-latest",
        "run",
        "dev"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run React 17",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "workspace",
        "react-17",
        "run",
        "dev"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "CourierJS Tests",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "workspace",
        "@trycourier/courier-js",
        "run",
        "test"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "CourierUIInbox Tests",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "workspace",
        "@trycourier/courier-ui-inbox",
        "run",
        "test"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Localhost QR",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "run",
        "ipqr"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Sync Packages",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "sync"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Build Packages",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "build-packages"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Example on Phone",
      "runtimeExecutable": "sh",
      "runtimeArgs": [
        "./scripts/ipqr.sh"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Setup Page for Screenshot",
      "runtimeExecutable": "sh",
      "runtimeArgs": [
        "./scripts/screenshot_launch.sh"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Generate Production API Doc (courier-js)",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "build-packages:ci",
        "&&",
        "yarn",
        "workspace",
        "@trycourier/courier-js",
        "run",
        "generate-api-doc"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}